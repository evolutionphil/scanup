{
  "cli": {
    "version": ">= 10.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://scanup-production.up.railway.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m1-medium"
      },
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://scanup-production.up.railway.app"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "resourceClass": "m-medium",
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_BACKEND_URL": "https://scanup-production.up.railway.app"
      },
      "node": "22.16.0"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "info@visiongo.at",
        "ascAppId": "6757320007",
        "appleTeamId": "M6T85HP76P",
        "ascApiKeyId": "DM6AAZ4JG8",
        "ascApiKeyIssuerId": "c065b0bb-35e7-46c8-8ab4-71d078008ebb",
        "ascApiKeyPath": "./AuthKey_DM6AAZ4JG8.p8"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
